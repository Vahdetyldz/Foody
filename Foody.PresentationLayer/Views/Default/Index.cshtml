
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_DefaultHeadSectionComponentPartial")

<body>
    <!-- Spinner Start -->
    <div id="spinner" class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
        <div class="spinner-border text-primary" role="status"></div>
    </div>
    <!-- Spinner End -->
    <!-- Navbar Start -->
    @await Component.InvokeAsync("_DefaultNavbarSectionComponentPartial")
    <!-- Navbar End -->

    <!-- Carousel Start -->
    @await Component.InvokeAsync("_DefaultCarouselSectionComponentPartial")
    <!-- Carousel End -->

    <!-- About Start -->
    @await Component.InvokeAsync("_DeafultAboutSectionComponentPartial")
    <!-- About End -->

    <!-- Feature Start -->
    @await Component.InvokeAsync("_DefaultFeatureSectionComponentPartial")
    <!-- Feature End -->

    <!-- Product Start -->
    @await Component.InvokeAsync("_DefaultProductSectionComponentPartial")
    <!-- Product End -->

    <!-- Firm Visit Start -->
    @await Component.InvokeAsync("_DeafultFirmSectionComponentPartial")
    <!-- Firm Visit End -->

    <!-- Testimonial Start -->
    @await Component.InvokeAsync("_DeafultTestimonialSectionComponentPartial")
    <!-- Testimonial End -->

    <!-- Blog Start -->
    @await Component.InvokeAsync("_DeafultBlogSectionComponentPartial")
    <!-- Blog End -->

    <!-- Footer Start -->
    @await Component.InvokeAsync("_DeafultFooterSectionComponentPartial")
    <!-- Footer End -->

    <!-- Back to Top -->
    <a href="#" class="btn btn-lg btn-primary btn-lg-square rounded-circle back-to-top"><i class="bi bi-arrow-up"></i></a>


    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="/foody2-1.0.0/lib/wow/wow.min.js"></script>
    <script src="/foody2-1.0.0/lib/easing/easing.min.js"></script>
    <script src="/foody2-1.0.0/lib/waypoints/waypoints.min.js"></script>
    <script src="/foody2-1.0.0/lib/owlcarousel/owl.carousel.min.js"></script>

    <!-- Template Javascript -->
    <script src="/foody2-1.0.0/js/main.js"></script>
</body>

</html>
